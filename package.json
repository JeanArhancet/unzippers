{
  "package": "unzippers",
  "version": "1.0.0",
  "description": "Fastest Unzip library in Node.js",
  "main": "index.js",
  "repository": "https://github.com/JeanArhancet/unzippers",
  "license": "MIT",
  "keywords": [
    "unzippers",
    "napi-rs",
    "Rust",
    "unzip",
    "archive"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "napi": {
    "name": "unzippers",
    "package": {
      "name": "unzippers"
    },
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "i686-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "arm-linux-androideabi",
        "x86_64-unknown-freebsd",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.9.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "adm-zip": "^0.5.9",
    "ava": "^4.3.0",
    "benny": "^3.7.1",
    "chalk": "^5.0.0",
    "esbuild": "^0.14.51",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "extract-zip": "^2.0.1",
    "glob": "^7.2.0",
    "husky": "^8.0.1",
    "node-stream-zip": "^1.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "pretty-bytes": "^6.0.0",
    "table": "^6.8.0",
    "typescript": "^4.7.3"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "bench": "yarn run build && node benches/bench.js",
    "build": "napi build --platform --release --pipe \"prettier -w\"",
    "build:debug": "napi build --platform --pipe \"prettier -w\"",
    "format": "run-p format:js format:rs",
    "format:js": "prettier . -w",
    "format:rs": "cargo fmt",
    "lint": "run-p lint:js lint:rs",
    "lint:js": "eslint . -c ./.eslintrc.yml",
    "lint:rs": "cargo clippy",
    "prepublishOnly": "napi prepublish -t npm && esbuild --minify --outfile=index.js --allow-overwrite index.js",
    "test": "run-p test:js",
    "test:js": "cd test && npx ava . && cd ..",
    "test:mem": "node memory-leak-detect.mjs",
    "version": "napi version"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "packageManager": "yarn@3.2.2"
}
